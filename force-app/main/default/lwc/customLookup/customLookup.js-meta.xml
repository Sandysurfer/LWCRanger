<?xml version="1.0" encoding="UTF-8" ?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>58.0</apiVersion>
    <isExposed>true</isExposed>
    <targets>
        <target>lightning__AppPage</target>
    </targets>
    <targetConfigs>
        <targetConfig targets="lightning__AppPage">
            <property
        name="apiName"
        type="String"
        required="true"
        default="Account"
        label="Enter Object Api Name"
      />
            <property
        name="objectLabel"
        type="String"
        required="true"
        default="Account"
        label="Enter Object Label"
      />
            <property
        name="iconName"
        type="String"
        required="true"
        default="standard:account"
        label="Icon for Object"
        description="Check slds for more info on icons"
      />
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>
